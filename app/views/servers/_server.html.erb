<div id="<%= dom_id server %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= server.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ip:</strong>
    <%= server.ip %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Region:</strong>
    <%= server.region.humanize %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Frabricated at:</strong>
    <%= server.frabricated_at %>
  </div>
</div>
